<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag(login_path) do %>
      <%= label_tag "email" %>
      <%= email_field_tag "email" %>
      <br/>
      <%= label_tag "password" %>
      <%= password_field_tag "password" %>
      <br/>
      <%= submit_tag "Log in" %>
    <% end %>
  </div>
</div>